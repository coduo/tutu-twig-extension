{
    "name": "coduo/tutu-twig-extension",
    "license": "MIT",
    "type": "project",
    "description": "Adds new features to TuTu twig templates engine.",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "require": {
        "php": ">=5.5.0",
        "twig/twig": "1.*"
    },
    "require-dev": {
        "behat/behat": "3.0.*",
        "behat/mink-extension": "~2.0",
        "behat/mink-goutte-driver": "~1.0",
        "bossa/phpspec2-expect": "~1.0",
        "coduo/tutu-faker-extension": "1.0.*",
        "coduo/tutu": "1.0.*"
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
